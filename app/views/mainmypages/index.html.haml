    %ul.mypage__userInfo__secondMenu__tabs
      %li.active
        %h3.mypage__userInfo__secondMenu__tabs__transactionNow  取引中
      %li
        %h3.mypage__userInfo__secondMenu__tabs__transactionOld  過去の取引
    .mypage__userInfo__secondMenu__contents
      %ul.mypage__userInfo__secondMenu__contents__notice
        %li.mypage__userInfo__secondMenu__contents__notice__sample
          %figure
          .mypage__userInfo__secondMenu__contents__notice__sample__text 取引中商品のサンプルです
          %i.fas.fa-chevron-right.fa-pull-right
        %li.mypage__userInfo__secondMenu__contents__notice__list 
