.wrapper
  .container
    = render '/devise/shared/user_header'

    .contents
      .contents__container

        %h2.contents__title
          新規会員登録
        .contents__main
          .contents__main__menu
            =link_to "#", class: "btn-default link-mail" do
              = icon('far', 'envelope', class: "mail-icon")
              メールアドレスで登録する
            =link_to "https://www.facebook.com/", class: "btn-default link-fb" do
              = icon('fab', 'facebook-square',  class: "fb-icon")
              Facebookで登録する
            =link_to "http://www.google.com/", class: "btn-default link-google" do
              = icon('fab', 'google', class: "google-icon")
              Googleで登録する

= render '/devise/shared/user_footer'